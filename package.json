{
    "name": "ihydra",
    "version": "5.2.0",
    "description": "IHydra is a Jupyter kernel for the Hydra video synth livecoding environment",
    "keywords": [
        "kernel",
        "ipython",
        "jupyter",
        "hydra",
        "video",
        "electron",
        "livecoding"
    ],
    "homepage": "https://github.com/jfhbrook/ihydra",
    "bugs": {
        "url": "https://github.com/jfhbrook/ihydra/issues"
    },
    "license": "BSD-3-Clause",
    "author": {
        "name": "Josh Holbrook",
        "email": "josh.holbrook@gmail.com",
        "url": "https://josh.agency"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jfhbrook/ihydra.git"
    },
    "main": "main.js",
    "bin": {
        "ihydra": "bin/ihydra.js",
        "ihydraconsole": "bin/ihydraconsole.js",
        "ihydrainstall": "bin/ihydrainstall.js",
        "ihydrakernel": "lib/kernel.js",
        "ihydranotebook": "bin/ihydranotebook.js"
    },
    "dependencies": {
        "app-root-path": "^3.0.0",
        "jp-kernel": "git+https://github.com/jfhbrook/jp-kernel.git#develop",
        "nel": "git+https://github.com/jfhbrook/nel.git#develop"
    },
    "devDependencies": {
        "debug": "2",
        "electron": "^7.1.10",
        "electron-mocha": "^8.2.0",
        "electron-rebuild": "^1.9.0",
        "electron-webpack": "^2.7.4",
        "eslint": "2",
        "jmp": "1 || 2",
        "jsdoc": "3",
        "mocha": "3",
        "uuid": "3",
        "webpack": "^4.41.5"
    },
    "engineStrict": true,
    "scripts": {
        "start": "electron .",
        "lint": "eslint bin lib test",
        "test:ihydrakernel": "electron-mocha test/ihydrakernel.js",
        "test": "npm run lint && npm run test:ihydrakernel",
        "compile": "electron-rebuild && electron-webpack",
        "dist": "npm run compile && electron-builder"
    },
    "build": {
        "appId": "com.electron.jfhbrook.ihydra",
        "productName": "IHydra",
        "mac": {
            "category": "public.app-category.graphics-design"
        }
    }
}
